# Database Configuration (MySQL 8.0.16)
DATABASE_URL=mysql+aiomysql://username:password@localhost:3306/investment_research
DATABASE_ECHO=false

# AI Service Configuration (SiliconCloud with DeepSeek-V3.2)
SILICONCLOUD_API_KEY=your_siliconcloud_api_key_here
SILICONCLOUD_MODEL=deepseek-ai/DeepSeek-V3.2
SILICONCLOUD_TEMPERATURE=0.7
SILICONCLOUD_MAX_TOKENS=4096
SILICONCLOUD_TOP_P=0.7
SILICONCLOUD_TOP_K=50
SILICONCLOUD_FREQUENCY_PENALTY=0.5
SILICONCLOUD_MIN_P=0.05
SILICONCLOUD_API_BASE=https://api.siliconflow.cn/v1

# LangChain Configuration
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_PROJECT=investment-research-reports

# MCP Search Configuration
MCP_SEARCH_ENDPOINT=http://localhost:8080
MCP_SEARCH_API_KEY=your_mcp_api_key_here

# Application Configuration
APP_NAME=Investment Research Reports System
APP_VERSION=0.1.0
DEBUG=false
LOG_LEVEL=INFO

# Security
SECRET_KEY=your_secret_key_here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# External Data Sources
BLOOMBERG_API_KEY=your_bloomberg_api_key_here
REUTERS_API_KEY=your_reuters_api_key_here
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Cache Configuration
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=3600

# Report Generation
REPORT_OUTPUT_DIR=./reports
TEMP_DIR=./temp
MAX_CONCURRENT_TASKS=5